{
    "name": "shipmonk/packing",
    "version": "0.1.0",
    "description": "Testing day application stub",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=8.4",
        "psr/http-message": "2.*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "2.*",
        "doctrine/orm": "3.*",
        "symfony/cache": "7.*",
        "symfony/dependency-injection": "7.*",
        "symfony/config": "7.*",
        "symfony/yaml": "7.*",
        "symfony/dotenv": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/property-access": "^7.3",
        "phpdocumentor/reflection-docblock": "^5.6",
        "dvdoug/boxpacker": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^2.1",
        "symplify/easy-coding-standard": "^12.6",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/unit"
        }
    },
    "scripts": {
        "phpstan": "php vendor/bin/phpstan analyse --memory-limit=512M --configuration=phpstan.neon",
        "ecs": "php vendor/bin/ecs --fix",
        "test": "php vendor/bin/phpunit tests/"
    }
}
